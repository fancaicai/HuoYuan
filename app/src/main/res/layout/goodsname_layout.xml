                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>

        </TableRow>

        <TableRow
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/jj_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="家具"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"
                />
            <TextView
                android:id="@+id/sm_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="树苗"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/hf_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="化肥"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/ls_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="粮食"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/gc_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="钢材"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
        </TableRow>
        <TableRow
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/pij_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="啤酒"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape1"
                />
            <TextView
                android:id="@+id/nn_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="牛奶"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape1"/>
            <TextView
                android:id="@+id/kqs_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="矿泉水"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape1"/>
            <TextView
                android:id="@+id/th_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="炭黑"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape1"/>
            <TextView
                android:id="@+id/qt_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="其他"
                android:gravity="center"
                android:textColor="@color/blue"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape"/>
        </TableRow>
    </TableLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/fahuo"
        />
    <TextView
        android:id="@+id/btncal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp"
        android:textSize="@dimen/backtxtsize"
        android:gravity="center"
        android:text="取消"/>
    </LinearLayout>
</LinearLayout>