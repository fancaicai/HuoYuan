<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/ffffff">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="货物类型："
        android:textColor="@color/hei"
        android:layout_marginTop="6dp"
        android:textSize="@dimen/backtxtsize"
        android:layout_marginLeft="@dimen/marginlefa"/>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:padding="6dp"
        >
        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/ph_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="普货"
                android:textColor="@color/hei"
                android:gravity="center"
                android:layout_weight="1"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="6dp"
                android:textSize="@dimen/backtxtsize"
                android:background="@drawable/goodname_layoutshape2"
                />
            <TextView
                android:id="@+id/zh_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="重货"
                android:textColor="@color/hei"
                android:gravity="center"
                android:layout_weight="1"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="6dp"
                android:textSize="@dimen/backtxtsize"
                android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/paoh_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="泡货"
                android:textColor="@color/hei"
                android:gravity="center"
                android:layout_weight="1"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="6dp"
                android:textSize="@dimen/backtxtsize"
                android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/sb_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="设备"
                android:textColor="@color/hei"
                android:gravity="center"
                android:layout_weight="1"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="6dp"
                android:textSize="@dimen/backtxtsize"
                android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/pj_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="配件"
                android:textColor="@color/hei"
                android:gravity="center"
                android:layout_weight="1"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:layout_marginLeft="6dp"
                android:layout_marginRight="6dp"
                android:textSize="@dimen/backtxtsize"
                android:background="@drawable/goodname_layoutshape2"/>

        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/bh_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="百货"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"
                />
            <TextView
                android:id="@+id/jc_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="建材"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/sp_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="食品"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/yl_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="饮料"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/hg_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="化工"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
        </TableRow>

        <TableRow
            android:layout_marginBottom="10dp">

            <TextView
                android:id="@+id/sg_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="水果"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"
                />
            <TextView
                android:id="@+id/sc_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="蔬菜"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/mc_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="木材"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
                android:id="@+id/mt_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="煤炭"
                android:textColor="@color/hei"
                android:gravity="center"
                 android:layout_weight="1"                 android:paddingTop="5dp"                 android:paddingBottom="5dp"                 android:layout_marginLeft="6dp"                 android:layout_marginRight="6dp"                 android:textSize="@dimen/backtxtsize"                 android:background="@drawable/goodname_layoutshape2"/>
            <TextView
